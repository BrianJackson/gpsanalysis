{
	"name": "items_to_items",
	"properties": {
		"activities": [
			{
				"name": "Copy_ao0",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "."
					},
					{
						"name": "Destination",
						"value": "items"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT [ItemId]\n      ,[VoteCount]\n      ,[ProductName]\n      ,[ImdbId]\n      ,i.[Description]\n      ,[ImagePath]\n      ,[ThumbnailPath]\n      ,[UnitPrice]\n      ,c.[CategoryId]\n      ,c.[CategoryName] as Category\n      ,[Popularity]\n      ,[OriginalLanguage]\n      ,[ReleaseDate]\n      ,[VoteAverage]\n      , 'Item' as Type\n  FROM [dbo].[Item] i\n  INNER JOIN dbo.Category c ON i.CategoryId = c.CategoryId",
						"partitionOption": "None"
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_ao0",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_ao0",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}