{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "brianjacDF"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"DataLakeKey_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeKey'"
		},
		"DataWarehouse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DataWarehouse'"
		},
		"TripsDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TripsDB'"
		},
		"DataLake KVAT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://brianjacdatalakegen2.dfs.core.windows.net"
		},
		"DataLakeKey_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://brianjacdatalakegen2.dfs.core.windows.net"
		},
		"DataWarehouse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "bjadmin"
		},
		"MLOps Key Vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mlopsbj-AML-KV.vault.azure.net/"
		},
		"Trigger_olb_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_olb_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "brianjacDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeKey_accountKey": {
						"value": "[parameters('DataLakeKey_accountKey')]"
					},
					"TripsDB_connectionString": {
						"value": "[parameters('TripsDB_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"DataWarehouse_connectionString": {
						"value": "[parameters('DataWarehouse_connectionString')]"
					},
					"DataLake KVAT_properties_typeProperties_url": {
						"value": "[parameters('DataLake KVAT_properties_typeProperties_url')]"
					},
					"DataLakeKey_properties_typeProperties_url": {
						"value": "[parameters('DataLakeKey_properties_typeProperties_url')]"
					},
					"MLOps Key Vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('MLOps Key Vault_properties_typeProperties_baseUrl')]"
					},
					"DataWarehouse_properties_typeProperties_userName": {
						"value": "[parameters('DataWarehouse_properties_typeProperties_userName')]"
					},
					"Trigger_olb_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_olb_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_olb_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_olb_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}